pipeline {
 agent any 
 
    stages {
     
      stage ('Preparando') {
        git branch: 'main', url: 'https://github.com/gustavokennedy/jenkins.git'
        sh 'npm install'
      }
     
        stage('Teste') {
            steps {
                sh 'ls'
            }
        }
        stage('Deliver') {
            steps {
                sh 'node --version'
            }
        }
     
   
     
    }
}
