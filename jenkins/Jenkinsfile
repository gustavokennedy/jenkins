pipeline {
 agent any 
 
    stages {
     
        stage('Logando'){
         steps{
            sshagent(credentials:['90173669-204f-456a-ab02-d1d9c81a784c']){
               sh 'ssh  -o StrictHostKeyChecking=no  ubuntu@18.215.143.84 uptime "whoami"'
          }
        echo "Logado com sucesso!"
         }
       }
   }

    }
